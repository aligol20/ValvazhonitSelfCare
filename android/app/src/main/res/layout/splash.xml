<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id='@+id/anan'
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eaefec"
    android:gravity="center"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/koala"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_weight="0.01"
        app:srcCompat="@mipmap/koala" />

    <TextView
        android:id="@+id/wellcome"
        android:layout_width="match_parent"
        android:layout_height="57dp"
        android:layout_weight="0.2"
        android:fontFamily="B Koodak"
        android:gravity="center"
        android:text="خوش آمدید"
        android:textColor="#4d13d1"
        android:textSize="19dp"
        tools:text="kkjkjkjk" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.6"
        app:srcCompat="@mipmap/self_care" />
    <ProgressBar
        android:id="@+id/progressBar"
        style="@android:style/Widget.DeviceDefault.Light.ProgressBar.Small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.1"
        android:indeterminateTint="#4d13d1" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.2"
        android:fontFamily="B Koodak"
        android:gravity="center"
        android:text="خود مراقبتی ولوواژینیت کاندیدایی"
        android:textColor="#4d13d1"
        android:textSize="23dp"
        tools:text="kkjkjkjk" />



</LinearLayout>
